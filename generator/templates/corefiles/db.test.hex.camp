$ORIGIN test.hex.camp.
@	3600 IN	SOA ns.test.hex.camp. hostmaster.test.hex.camp. (
				{{ serial }} ; serial
				7200       ; refresh (2 hours)
				3600       ; retry (1 hour)
				1209600    ; expire (2 weeks)
				300       ; minimum (5 minutes)
				)

{%- if ips.minikube9 %}
@ 3600 IN NS ns-minikube9
ns-minikube9	300 A {{ ips.minikube9 }}
ns1	300 A {{ ips.minikube9 }}
{%- endif %}

{%- if ips.minikube10 %}
@ 3600 IN NS ns-minikube10
ns-minikube10	300 A {{ ips.minikube10 }}
ns2	300 A {{ ips.minikube10 }}
{%- endif %}

{%- if ips.minikube11 %}
@ 3600 IN NS ns-minikube11
ns-minikube11	300 A {{ ips.minikube11 }}
ns3	300 A {{ ips.minikube11 }}
{%- endif %}

